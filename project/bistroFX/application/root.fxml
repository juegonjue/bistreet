<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.*?>
<?import javafx.geometry.*?>

<StackPane prefHeight="500" prefWidth="350" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8"
 fx:controller="application.MainController">
    <children>
        <BorderPane>
            <top>
                <BorderPane style="-fx-background-color: #eaeaea;">
                    <center>            
                        <Label alignment="CENTER" prefWidth="215" text="메인 화면" />
                    </center>
                    
                    <right>
                        <Button fx:id="btnLogin" text="로그인" />
                    </right>
                    <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                    </padding>
                </BorderPane>
            </top>
            
           <center>
              <VBox style="-fx-background-color: #0000ff;">
               <children>
                  <Button fx:id="btnmeal" mnemonicParsing="false" text="식사" />
                  <Button fx:id="btndesert" mnemonicParsing="false" text="디저트" />
                  <Button fx:id="btnpup" mnemonicParsing="false" text="주점" />
               </children></VBox>
           </center>        
        </BorderPane>
    </children>
</StackPane>
